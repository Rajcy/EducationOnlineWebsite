<script src="js/jquery.min.js"></script>
<script src="js/idangerous.swiper2.7.6.min.js"></script>
<script src="js/swiper.animate1.0.2.min.js"></script>
<script>

    var mySwiper = new Swiper('.swiper-container', {
        pagination: '.pagination',
        paginationClickable: true,
        autoplay: 5000,
        speed: 1,
        loop: true,
        observer: true, //修改swiper自己或子元素时，自动初始化swiper
        observeParents: true,
        // autoplayDisableOnInteraction : false,//强大的swiper

        onInit: function (swiper) { //Swiper2.x的初始化是onFirstInit
            swiperAnimateCache(swiper); //隐藏动画元素
            swiperAnimate(swiper); //初始化完成开始动画
        },
        onSlideChangeEnd: function (swiper) {
            swiperAnimate(swiper); //每个slide切换结束时也运行当前slide动画
        }
    })
    $('.arrow-left').on('click', function (e) {
        e.preventDefault()
        mySwiper.swipePrev()
    })
    $('.arrow-right').on('click', function (e) {
        e.preventDefault()
        mySwiper.swipeNext()
    })
</script>
<script>
    $(".lanren ul li").hover(function () {
        $(this).find(".txt").stop().animate({height: "150px"}, 400);
        $(this).find(".txt h3").stop().animate({paddingTop: "60px"}, 400);
        $(this).find(".txt h3").stop().css({'text-decoration': 'underline'});
    }, function () {
        $(this).find(".txt").stop().animate({height: "35px"}, 400);
        $(this).find(".txt h3").stop().animate({paddingTop: "0px"}, 400);
        $(this).find(".txt h3").stop().css({'text-decoration': 'none'});
    })

    /*获取屏幕
     $(document).ready(function(){
     var screenWidth = $(window).width();//获取屏幕可视区域的宽度。
     $(".bottomDiv").width(screenWidth);//将宽度赋值给bottomDiv使其可以贯穿整个屏幕。
     var screenHeight = $(window).height();//获取屏幕可视区域的高度。
     var divHeight = $(".bottomDiv").height();//bottomDiv的高度再加上它一像素的边框。

     $(window).scroll(function(){
     var scrollHeight = $(document).scrollTop();//获取滚动条滚动的高度。
     if(!window.XMLHttpRequest){
     $(".bottomDiv").css("top",screenHeight + scrollHeight - divHeight);
     }//判断是否为IE6，如果是，执行大括号中内容
     })*/


</script>
<!--这里是触摸时结束-->

<script type="text/javascript">

    $(document).ready(function () {

        //alert($(window).width());

//    <!---------判断是手机页面还是pc页面------------>
        //平台、设备和操作系统,  判断是否为手机打开网页
        var system = {
            win: false,
            mac: false,
            xll: false
        };
//检测平台
        var p = navigator.platform;
        system.win = p.indexOf("Win") == 0;
        system.mac = p.indexOf("Mac") == 0;
        system.x11 = (p == "X11") || (p.indexOf("Linux") == 0);
//跳转语句，如果是手机访问就修改样式
        if (system.win || system.mac || system.xll) {
            $(".swiper-container").css("width", "100%");
            $("#i_fine").css("width", "100%");
            $("#i_teacher").css("width", "100%");
            $("#i_judge_pic").css("width", "100%");
            $(".i_link").css("width", "100%");
            $(".i_link2").css("width", "100%");
            $(" .bottomDiv").css("width", "100%");

            if (system.win) {
                //配置
                var config = {
                    vx: 4,//点x轴速度,正为右，负为左
                    vy: 4,//点y轴速度
                    height: 2,//点高宽，其实为正方形，所以不宜太大
                    width: 2,
                    count: 200,//点个数
                    color: "248, 44, 68",//点颜色
                    stroke: "248, 44, 68",//线条颜色
                    dist: 6000,//点吸附距离
                    e_dist: 20000,//鼠标吸附加速距离
                    max_conn: 10//点到点最大连接数
                }
                //调用
                CanvasParticle(config);
            }
        } else {
            $(".detailViewWrapper").css({"margin-left": "25px"});
            $("#dialog_show").click();
            $("#dialog_details").html("检测到当前终端为手机/平板，是否前往浏览移动端官网");
            $("#login_sure").click(function () {
                window.location.replace("http://extend.xueshenx.com");
            });
            $(".dialog__overlay").click(function () {
                return true;
                //alert(config+phone)
            });
            //window.location.replace("http://extend.xueshenx.com");
            $(".swiper-container").css("width", "105%");
            $("#i_fine").css("width", "105%");
            $("#i_teacher").css("width", "105%");
            $("#i_judge_pic").css("width", "105%");
            $(".i_link").css("width", "105%");
            $(".i_link2").css("width", "100%");
            $(" .bottomDiv").css("width", "105%");

        }


        var storage = window.localStorage;
        var config = storage.getItem("token");
        var phone = storage.getItem("username");
        if (config != "" && phone != null) {
            $('.i_right_login a').html("&nbsp;登出&nbsp;");
            $('.i_right_personal').css("display", "inline");
            console.dir(config);
        }
        else {
            $('.i_right_login a').html("&nbsp;注册/登录&nbsp;");
        }

    });
</script>

<script type="text/javascript">
    $('.i_right_login a').click(function () {
        var storage = window.localStorage;
        var config = storage.getItem("token");
        if (config != null) {
            $.post("http://api.xueshenx.com/common/logout", {"token": config}, function (data) {
                switch (data.resultData.code) {
                    case 200:
                        $('.i_right_login a').html("注册/登录");
                        localStorage.removeItem('token');
                        localStorage.removeItem('username');
                        window.location.href = 'login.html';
                        break;
                    case 201:
                        alert("退出登录失败");
                        break;
                }
            });

        }
        else {
            //alert("你还未登录，请先登录");
            window.location.href = 'login.html';
        }
    });

    $('#personal_center').click(function () {
        window.location.href = 'resume.html';
    });
</script>
